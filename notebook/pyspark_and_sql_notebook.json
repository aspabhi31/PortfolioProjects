				"source": [
					"%%pyspark\n",
					"blob_account_name = \"abhijeetsinghpawar\"\n",
					"blob_container_name = \"abhiblob\"\n",
					"from pyspark.sql import SparkSession\n",
					"\n",
					"sc = SparkSession.builder.getOrCreate()\n",
					"token_library = sc._jvm.com.microsoft.azure.synapse.tokenlibrary.TokenLibrary\n",
					"blob_sas_token = token_library.getConnectionString(\"AzureBlobStorage1\")\n",
					"\n",
					"spark.conf.set(\n",
					"    'fs.azure.sas.%s.%s.blob.core.windows.net' % (blob_container_name, blob_account_name),\n",
					"    blob_sas_token)\n",
					"df = spark.read.load('wasbs://abhiblob@abhijeetsinghpawar.blob.core.windows.net/Products.csv', format='csv',\n",
					"header=True\n",
					")\n",
					"display(df.limit(10))"
				]
			},
